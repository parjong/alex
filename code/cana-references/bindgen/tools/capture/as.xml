<actions>
  <consume>
    <name>consume_1</name>
    <offset>1</offset>
  </consume>

  <consume>
    <name>consume_2</name>
    <offset>2</offset>    
  </consume>

  <consume>
    <name>output</name>
    <offset>2</offset>    
    <consumer>append</consumer>
  </consume>

  <match>
    <name>source</name>
    <regex>([^-].*)</regex>
    <matcher>append</matcher>
  </match>
</actions>

<scanner>
  <must>
    <value>--64</value>
    <action>consume_1</action>
  </must>

  <must>
    <value>-o</value>
    <action>output</action>
  </must>

  <may>
    <action>source</action>
  </may>
</scanner>
